{
    
    "builds":[
        {
            "src": "to_do_list/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.10"}
        }
        
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "to_do_list/wsgi.py"
        }
    ]
}

